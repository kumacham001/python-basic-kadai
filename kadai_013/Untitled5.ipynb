        "#戻り値：税込金額\n",
        "def calculate_tax(goods, tax_rate):\n",
        "        return goods*(tax_rate/100+1)\n",
        "\n",
        "#第1引数:金額、第2引数：消費税税率(%)、\n",
        "total=calculate_tax(1980,10)\n",
        "print(total)\n"
